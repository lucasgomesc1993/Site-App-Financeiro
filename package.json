{
  "name": "finzap",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run build:client && npm run build:server && npm run prerender && npm run sitemap",
    "build:client": "vite build --outDir dist/static",
    "build:server": "vite build --ssr entry-server.tsx --outDir dist/server",
    "prerender": "node scripts/prerender.js",
    "serve:static": "npx serve dist/static",
    "preview": "vite preview",
    "sitemap": "node scripts/generate-sitemap.js"
  },
  "dependencies": {
    "@google/genai": "^1.30.0",
    "framer-motion": "^12.23.24",
    "lucide-react": "^0.555.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-helmet-async": "^2.0.5",
    "react-router-dom": "^7.9.6"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.17",
    "@types/node": "^22.14.0",
    "@vitejs/plugin-react": "^5.0.0",
    "autoprefixer": "^10.4.22",
    "postcss": "^8.5.6",
    "sharp": "^0.34.5",
    "tailwindcss": "^4.1.17",
    "typescript": "~5.8.2",
    "vite": "^6.2.0"
  },
  "overrides": {
    "react-helmet-async": {
      "react": "$react",
      "react-dom": "$react-dom"
    }
  }
}